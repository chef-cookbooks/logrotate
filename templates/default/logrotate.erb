<% @path.each do |p| -%>
<%= p %> {
  <%= @frequency %>
  missingok
  rotate <%= @rotate %>
  compress
  delaycompress
  copytruncate
  notifempty
  create <%= @create %>
<%- if @sharedscripts %>
  sharedscripts
<%- end %>
<%- if @postrotate %>
  postrotate
<%= @postrotate %>
  endscript
<%- end %>
<%- if @prerotate %>
  prerotate
<%= @prerotate %>
  endscript
<%- end %>
}
<% end -%>
